{
  "name": "MatzipApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ios:dev": "react-native run-ios --scheme Develop",
    "ios:prod": "react-native run-ios --scheme Production",
    "ios:repod": "cd ios && rm -rf Pods build Podfile.lock && pod deintegrate && pod install --repo-update && cd ..",
    "android:dev": "ENVFILE=./env/.env.development && react-native run-android",
    "android:prod": "ENVFILE=./env/.env.production && react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.2.10",
    "@tanstack/react-query": "^5.74.7",
    "axios": "^1.9.0",
    "react": "18.3.1",
    "react-native": "0.76.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-maps": "^1.8.3",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.0",
    "@react-native-community/cli-platform-android": "15.0.0",
    "@react-native-community/cli-platform-ios": "15.0.0",
    "@react-native/babel-preset": "0.76.1",
    "@react-native/eslint-config": "0.76.1",
    "@react-native/metro-config": "0.76.1",
    "@react-native/typescript-config": "0.76.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-config": "^1.5.5",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
